{
  "manifest_version": 3,
  "name": "GeminiTeams",
  "version": "2.0",
  "description": "Orchestrate a team of Gemini specialists with internal storage.",
  "permissions": ["sidePanel", "storage", "unlimitedStorage", "scripting", "activeTab"],
  "host_permissions": ["https://gemini.google.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "action": {
    "default_title": "Open GeminiTeams"
  }
}